<%# edit.erb %>
<!DOCTYPE html>
<html lang="en" dir="ltr">
  <head>
    <meta charset="utf-8">
    <title>Lists of Pets</title>
  </head>
  <body>
    <ul>
        <li><a href="/">HOME</a></li>
        <li><a href="/owners">OWNERS</a></li>
        <li><a href="/pets">PETS</a></li>
    </ul>
    <h1>Pets list</h1>
    <% @pets.each do |pet|%>
    <ul>
      <li><a href="/pets/<%= pet.id %>"><%= pet.name %></a> </li>
    </ul>
    <% end %>
    <a href="/pets/new">Create a new Pet</a>
  </body>
</html>
